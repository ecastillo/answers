{
  "name": "answers",
  "version": "1.6.3",
  "description": "Cascading Config Provider",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "test": "./node_modules/.bin/mocha \"test/**/*.spec.js\"",
    "watch": "npm run test -- --watch",
    "coverage": "rm -rf coverage && node node_modules/.bin/istanbul --include-all-sources cover -x \"**/test/**/*.spec.js\" -x \"**/examples/**\" --dir=\"coverage\" --root=\"./\" node_modules/.bin/_mocha -- \"test/**/*.spec.js\"",
    "check-coverage": "npm run coverage && ./node_modules/.bin/istanbul check-coverage --root=\"./coverage\" --statement 93 --function 100 --branch 60"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/machellerogden/answers.git"
  },
  "author": {
    "name": "Mac Heller-Ogden",
    "email": "mac@machellerogden.com"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "bluebird": "^3.5.0",
    "deepmerge": "^2.1.1",
    "ini": "^1.3.4",
    "inquirer": "^3.0.6",
    "lodash": "^4.17.10",
    "minimist": "^1.2.0",
    "parent-module": "^0.1.0",
    "read-pkg-up": "^3.0.0",
    "strip-json-comments": "^2.0.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.19.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "pre-commit": "^1.2.2",
    "proxyquire": "^1.7.11",
    "sinon": "^2.2.0",
    "sinon-chai": "^2.10.0"
  },
  "pre-commit": [
    "lint",
    "test",
    "check-coverage"
  ]
}
